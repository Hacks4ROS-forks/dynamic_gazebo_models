<?xml version="1.0"?>
<launch>

  <include file="$(find dynamic_models)/launch/empty_world.launch">
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="false"/>
    <arg name="world_name" value="$(find dynamic_models)/worlds/empty.world"/>
  </include>

  <!-- Flip Door Left -->

  <param name="spawn_door_1" command="$(find xacro)/xacro.py $(find dynamic_models)/models/flip_door_right.sdf" />

  <node pkg="gazebo_ros" type="spawn_model" name="gazebo_door1" args="-sdf -param spawn_door_1 -model flip_door_1 -x -3.042358 -y 5.532777 -z 0.3" respawn="false" output="screen">
  </node>

  <!-- Flip Door Right -->

  <param name="spawn_door_2" command="$(find xacro)/xacro.py $(find dynamic_models)/models/flip_door_left.sdf" />

  <node pkg="gazebo_ros" type="spawn_model" name="gazebo_door2" args="-sdf -param spawn_door_2 -model flip_door_2 -x -4.452335 -y 5.078486 -z 0.3" respawn="false" output="screen">
  </node>

  <include file="$(find sesto_navigation)/launch/sestos.launch">

    <arg name="commandTopic" value="cmd_vel"/>
    <arg name="odometryTopic" value="odom"/>
    
    <arg name="odometryFrame" value="odom"/>
    <arg name="robotBaseFrame" value="sesto_frame"/>
    <arg name="odometryRate" value="20.000000"/>
    
    <arg name="alpha1" value="0.000000"/>
    <arg name="alpha2" value="0.000000"/>
    <arg name="alpha3" value="0.000000"/>
    <arg name="alpha4" value="0.000000"/>
    <arg name="alpha5" value="0.000000"/>

  </include>

</launch>